{
  "name": "vueflix",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src \"**/*.{ts,tsx,vue}\" --no-error-on-unmatched-pattern ",
    "lint:fix": "eslint src \"**/*.{ts,tsx,vue}\" --fix --no-error-on-unmatched-pattern ",
    "format": "npx prettier \"src/**/*.{js,jsx,ts,tsx,html,css,scss,vue}\" --write",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@eslint/js": "^9.8.0",
    "@pinia/testing": "^0.1.5",
    "@storybook/manager-api": "^8.2.8",
    "@storybook/theming": "^8.2.8",
    "@types/node": "^22.1.0",
    "@vue/test-utils": "^2.4.6",
    "axios": "^1.7.3",
    "pinia": "^2.2.1",
    "sass-embedded": "^1.77.8",
    "swiper": "^11.1.9",
    "typescript-eslint": "^8.0.1",
    "vue": "^3.4.35",
    "vue-axios": "^3.5.2",
    "vue-router": "4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/addon-onboarding": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/test": "^8.2.8",
    "@storybook/vue3": "^8.2.8",
    "@storybook/vue3-vite": "^8.2.8",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.8.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^8.0.0",
    "jsdom": "^24.1.1",
    "prettier": "^3.3.3",
    "storybook": "^8.2.8",
    "storybook-vue3-router": "^5.0.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.0",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
